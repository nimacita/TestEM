				float dither13 = Dither8x8Bayer( fmod(clipScreen13.x, 8), fmod(clipScreen13.y, 8) );
				clip( _Alpha - dither13);
				
				fixed4 c = tex2DNode7;
				c.rgb *= c.a;
				return c;
			}
		ENDCG
		}
